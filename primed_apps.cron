# PRIMED_APPS crontab
# nightly except sunday at 01:00
0 1 * * MON-SAT . /var/www/django/primed_apps/primed-apps-activate.sh; python manage.py run_anvil_audit --email primedconsortium@uw.edu --errors-only >> cron.log 2>&1

# sunday night at 01:00
0 1 * * SUN . /var/www/django/primed_apps/primed-apps-activate.sh; python manage.py run_anvil_audit --email primedconsortium@uw.edu >> cron.log 2>&1
